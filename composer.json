{
    "name": "valeriy-brunov/insert-paginator",
    "description": "Создаёт AJAX-пагинацию или вставку в нужное место AJAX-контента.",
    "type": "cakephp-plugin",
    "home": "https://github.com/valeriy-brunov/insert-paginator",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "4.4.*",
        "valeriy-brunov/extensions": "^1.0",
        "valeriy-brunov/web-component": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "Insert-paginator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Insert-paginator\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
